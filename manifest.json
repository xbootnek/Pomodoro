{
  "manifest_version": 3,
  "name": "My Pomodoro",
  "version": "4.0",
  "description": "A Pomodoro timer extension with 1-minute work and break intervals.",
  "permissions": ["storage", "notifications"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "icon16.png"
    }
}
}